{
    "name": "yunwuxin/think-social",
    "description": "ThinkPHP6 Social SDK",
    "type": "think-extend",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "yunwuxin",
            "email": "448901948@qq.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3|^7.2",
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "yunwuxin\\": "src"
        },
        "files": [
            "src/common.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "yunwuxin\\social\\Service"
            ],
            "config": {
                "social": "src/config.php"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}